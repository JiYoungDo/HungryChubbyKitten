<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/mink"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/main_tv_timer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="time"
        android:textSize="40sp"
        android:textColor="@color/black"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="30dp"
        android:layout_marginRight="20dp"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/main_tv_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="30dp"
        android:text="Score"
        android:textColor="@color/red"
        android:textSize="35sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText,MissingConstraints" />


    <ImageView
        android:id="@+id/main_iv_cat_1"
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="@id/main_iv_cat_2"
        app:layout_constraintTop_toTopOf="@id/main_iv_cat_2"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/main_iv_cat_2"
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="110dp"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/main_iv_cat_3"
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:layout_constraintStart_toStartOf="@id/main_iv_cat_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/main_iv_cat_2"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/main_iv_cat_4"
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:layout_constraintStart_toStartOf="@id/main_iv_cat_1"
        app:layout_constraintEnd_toEndOf="@id/main_iv_cat_2"
        app:layout_constraintTop_toBottomOf="@id/main_iv_cat_2"
        android:layout_marginTop="35dp"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/main_iv_cat_5"
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:layout_constraintStart_toStartOf="@id/main_iv_cat_2"
        app:layout_constraintEnd_toEndOf="@id/main_iv_cat_3"
        app:layout_constraintTop_toTopOf="@id/main_iv_cat_4"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/main_iv_cat_6"
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="@id/main_iv_cat_2"
        app:layout_constraintTop_toBottomOf="@id/main_iv_cat_4"
        android:layout_marginTop="35dp"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/main_iv_cat_7"
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/main_iv_cat_6"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/main_iv_cat_8"
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:layout_constraintStart_toStartOf="@id/main_iv_cat_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/main_iv_cat_6"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/main_tv_explain"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="------------- 내 우유 -------------"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@id/main_iv_milk_1"
        app:layout_constraintTop_toBottomOf="@id/main_iv_cat_8"
        android:layout_marginBottom="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="HardcodedText" />
    <ImageView
    android:id="@+id/main_iv_milk_1"
    android:layout_width="20dp"
    android:layout_height="40dp"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintBottom_toTopOf="@id/main_fill_btn"
        android:layout_marginLeft="10dp"
        app:layout_constraintTop_toBottomOf="@+id/main_tv_explain"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/main_iv_milk_2"
        android:layout_width="20dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toRightOf="@id/main_iv_milk_1"
        app:layout_constraintRight_toLeftOf="@id/main_iv_milk_3"
        app:layout_constraintTop_toTopOf="@+id/main_iv_milk_1"
        app:layout_constraintBottom_toBottomOf="@+id/main_iv_milk_1"
        tools:ignore="ContentDescription,RtlHardcode" />

    <ImageView
        android:id="@+id/main_iv_milk_3"
        android:layout_width="20dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toRightOf="@id/main_iv_milk_1"
        app:layout_constraintRight_toLeftOf="@id/main_iv_milk_5"
        app:layout_constraintTop_toTopOf="@+id/main_iv_milk_1"
        app:layout_constraintBottom_toBottomOf="@+id/main_iv_milk_1"
        tools:ignore="ContentDescription,RtlHardcoded" />

    <ImageView
        android:id="@+id/main_iv_milk_4"
        android:layout_width="20dp"
        android:layout_height="40dp"
        app:layout_constraintBottom_toBottomOf="@+id/main_iv_milk_1"
        app:layout_constraintLeft_toRightOf="@id/main_iv_milk_3"
        app:layout_constraintRight_toLeftOf="@id/main_iv_milk_5"
        app:layout_constraintTop_toTopOf="@+id/main_iv_milk_1"
        tools:ignore="ContentDescription,RtlHardcoded" />

    <ImageView
        android:id="@+id/main_iv_milk_5"
        android:layout_width="20dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toLeftOf="@id/main_iv_cat_2"
        app:layout_constraintTop_toTopOf="@+id/main_iv_milk_1"
        app:layout_constraintBottom_toBottomOf="@+id/main_iv_milk_1"
        tools:ignore="ContentDescription,RtlHardcoded" />

    <ImageView
        android:id="@+id/main_iv_milk_6"
        android:layout_width="20dp"
        android:layout_height="40dp"
        app:layout_constraintRight_toRightOf="@id/main_iv_cat_2"
        app:layout_constraintTop_toTopOf="@+id/main_iv_milk_1"
        app:layout_constraintBottom_toBottomOf="@+id/main_iv_milk_1"
        tools:ignore="ContentDescription,RtlHardcoded" />

    <ImageView
        android:id="@+id/main_iv_milk_7"
        android:layout_width="20dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toRightOf="@id/main_iv_milk_6"
        app:layout_constraintRight_toLeftOf="@id/main_iv_milk_8"
        app:layout_constraintTop_toTopOf="@+id/main_iv_milk_1"
        app:layout_constraintBottom_toBottomOf="@+id/main_iv_milk_1"
        tools:ignore="ContentDescription,RtlHardcoded" />

    <ImageView
        android:id="@+id/main_iv_milk_8"
        android:layout_width="20dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toRightOf="@id/main_iv_milk_6"
        app:layout_constraintRight_toLeftOf="@id/main_iv_milk_10"
        app:layout_constraintTop_toTopOf="@+id/main_iv_milk_1"
        app:layout_constraintBottom_toBottomOf="@+id/main_iv_milk_1"
        tools:ignore="ContentDescription,RtlHardcoded" />

    <ImageView
        android:id="@+id/main_iv_milk_9"
        android:layout_width="20dp"
        android:layout_height="40dp"
        app:layout_constraintLeft_toRightOf="@id/main_iv_milk_8"
        app:layout_constraintTop_toTopOf="@+id/main_iv_milk_1"
        app:layout_constraintBottom_toBottomOf="@+id/main_iv_milk_1"
        app:layout_constraintRight_toLeftOf="@id/main_iv_milk_10"
        tools:ignore="ContentDescription,RtlHardcoded" />

    <ImageView
        android:id="@+id/main_iv_milk_10"
        android:layout_width="20dp"
        android:layout_height="40dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="10dp"
        app:layout_constraintTop_toTopOf="@+id/main_iv_milk_1"
        app:layout_constraintBottom_toBottomOf="@+id/main_iv_milk_1"
        android:layout_marginLeft="15dp"
        tools:ignore="ContentDescription,RtlHardcoded" />

    <Button
        android:id="@+id/main_fill_btn"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="우유 채우기"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="30dp"
        tools:ignore="HardcodedText,RtlHardcoded" />


</androidx.constraintlayout.widget.ConstraintLayout>